{
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/static-build"
    }
  ],
  "rewrites": [
    {
      "source": "/(img/.*)",
      "destination": "/dist/img/$1"
    },
    {
      "source": "/favicon.ico",
      "destination": "/dist/favicon.ico"
    },
    {
      "source": "/programs",
      "destination": "/dist/index.html"
    },
    {
      "source": "/keys",
      "destination": "/"
    },
    {
      "source": "/(.*)\\.(js|css|html|ico|json|wasm)",
      "destination": "/dist/$1.$2"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/index.html"
    }
  ]
}
